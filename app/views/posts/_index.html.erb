<table class="table table-hover table-inverse">
	<thead>
		<tr>
			<th>名前</th>
			<th>タイトル</th>
			<th></th>
			<th>タグ</th>
		</tr>
	</thead>

	<tbody>
		<% posts.each do |post| %>
		  <tr class="post_<%= post.id %>">
			  <%= render '/posts/post', post: post %>
		  </tr>
		<% end %>
	</tbody>
</table>
