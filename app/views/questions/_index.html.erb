<table class="table table-hover table-inverse">
	<thead>
		<tr>
			<th class="th-01">名前</th>
			<th class="th-04">タイトル</th>
			<th class="th-05"></th>
			<th class="th-02">タグ</th>
		</tr>
	</thead>

  <tbody>
		<% questions.each do |question| %>
		  <tr class="question_<%= question.id %>">
			  <%= render '/questions/question', question: question %>
		  </tr>
		<% end %>
  </tbody>
</table>
