<div class="container">
	<div class="row">
		<div class="col-xs-12 col-md-8 col-md-offset-4 col-sm-8 col-sm-offset-4 col-lg-8 col-lg-offset-4">
			<h2>質問ランキング（共感した順）</h2>
			<% @empathy_ranks.each.with_index(1) do |question, i| %>
				<div>第<%= i %>
				   位
				   <div class="question_<%= question.id %>">
				     <%= render 'questions/question', question: question %>
				   </div>
			  </div>
			<% end %>
　　</div>
	</div>
</div>